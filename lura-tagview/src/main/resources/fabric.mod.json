{
    "schemaVersion": 1,
    "version": "2001.0.0",

    "id": "luratagview",
    "name": "Tag Viewer",
    "description": "Just appends the lists of tags",
    "authors": [
        {
            "name": "Lura Skye"
        }
    ],
    "license": ["MPL-2.0"],
    "contact": {
        "issues": "https://github.com/Fuyukai/Lura-Mods"
    },

    "environment": "client",
    "entrypoints": {
        "client": [
            {
                "value": "tf.veriny.mc.tags.MakeUp",
                "adapter": "kotlin"
            }
        ]
